<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<package name="norm_default" extends="struts-default" >
		<action name="normquery" class="pdp.norm.action.NormAction">
			<result name="success">/page/norm/norm.jsp</result>
		</action>
		<action name="beforenormadd" class="pdp.norm.action.BeforeNormAdd">
			<result name="success">/page/norm/normadd.jsp</result>
		</action>
		<action name="normadd" class="pdp.norm.action.NormAdd">
			<result name="success">/page/norm/normsuccess.jsp</result>
		</action>
		<action name="normdel" class="pdp.norm.action.NormDel">
			<result name="success">/page/norm/normsuccess.jsp</result>
		</action>
		<action name="normtoupdate" class="pdp.norm.action.NormToUpdate">
			<result name="success">/page/norm/normupdate.jsp</result>
		</action>
		<action name="normupdate" class="pdp.norm.action.NormUpdate">
			<result name="success">/page/norm/normsuccess.jsp</result>
		</action>
		<action name="exportnorm" class="pdp.norm.action.ExportNorm">
			<result name="success">/page/public/derivedetail.jsp</result>
		</action>
	</package>
</struts>    
