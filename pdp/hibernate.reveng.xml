<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="user_info" catalog="pdp" class="UserInfo">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="user_pos" catalog="pdp" class="UserPos"></table>
	<table name="p_score" catalog="pdp" class="Score">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
<table name="p_position" catalog="pdp" class="Position">
	<primary-key>
		<generator class="native"></generator>
	</primary-key>
</table>
<table name="p_norm" catalog="pdp" class="Norm">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="p_flag" catalog="pdp" class="Flag">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
</hibernate-reverse-engineering>